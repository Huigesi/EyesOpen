<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SubsamplingScaleImageView">
        <attr name="src" format="reference"/>
        <attr name="assetName" format="string"/>
        <attr name="panEnabled" format="boolean"/>
        <attr name="zoomEnabled" format="boolean"/>
        <attr name="quickScaleEnabled" format="boolean"/>
        <attr name="tileBackgroundColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="GifView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomTheme">
        <attr name="gifViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="channel_tag_style">
        <attr name="channelItemBg" format="reference|integer" />
        <attr name="channelItemDragingBg" format="reference|integer" />
        <attr name="channelItemFixedBg" format="reference|integer" />
        <attr name="showPathAnim" format="boolean" />
        <attr name="fixedPos" format="integer" />
        <attr name="addedCategroyTitleBg" format="reference|color|integer" />
        <attr name="unAddedCategroyTitleBg" format="reference|color|integer" />
        <attr name="colums" format="integer" />
        <attr name="columnHorizontalSpace" format="integer|reference|dimension" />
        <attr name="columnVerticalSpace" format="integer|reference|dimension" />
        <attr name="channelItemTxColor" format="color|reference" />
        <attr name="channelItemTxSize" format="reference|dimension" />
        <attr name="itemDrawableLeft" format="reference|integer" />
    </declare-styleable>
    <declare-styleable name="SwipeBackLayout">
        <attr name="edge_size" format="dimension"/>
        <attr name="edge_flag">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
            <enum name="all" value="3" />
        </attr>
        <attr name="shadow_left" format="reference"/>
        <attr name="shadow_right" format="reference"/>
        <attr name="shadow_bottom" format="reference"/>
    </declare-styleable>

    <attr name="SwipeBackLayoutStyle" format="reference"/>
</resources>